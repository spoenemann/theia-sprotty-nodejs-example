{
    "compilerOptions": {
        "skipLibCheck": true,
        "strict": true,
        "noUnusedLocals": true,
        "noEmitOnError": false,
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "resolveJsonModule": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "jsx": "react"
    }
}
